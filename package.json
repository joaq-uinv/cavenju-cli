{
  "name": "cavenju-cli",
  "version": "0.1.1",
  "description": "CLI for Cavenju",
  "repository": "https://github.com/joaq-uinv/cavenju-cli.git",
  "main": "dist/index.js",
  "bin": {
    "cavenju": "dist/index.js"
  },
  "scripts": {
    "test": "jest --watchAll --coverage",
    "start": "node dist/index.js",
    "build": "tsc && npx pkg -t node14-linux-x64,node14-macos-x64,node14-win-x64 . --out-path dist",
    "package": "pkg . --targets node14-macos-x64 --output cavenju",
    "prepare": "npm run build"
  },
  "pkg": {
    "scripts": "dist/**/*.js"
  },
  "author": "Joaqu√≠n Villanueva",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.0",
    "inquirer": "^8.2.0",
    "mongoose": "^6.2.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.20",
    "jest": "^28.1.0",
    "pkg": "^5.5.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.5.5"
  }
}
